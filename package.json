{
  "name": "React-Express-Boilerplate",
  "version": "1.0.0",
  "description": "React and Express boilerplate",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "David Urbina",
  "license": "MIT",
  "dependencies": {
    "async": "^2.0.1",
    "babel-polyfill": "6.8.0",
    "bcrypt": "^0.8.6",
    "body-parser": "^1.15.2",
    "bootstrap": "3.3.6",
    "bootstrap-material-design": "^0.5.10",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "jquery": "2.2.3",
    "json-loader": "^0.5.4",
    "json-web-token": "^2.0.2",
    "moment": "^2.13.0",
    "mongoose": "^4.5.3",
    "morgan": "^1.7.0",
    "multer": "^1.1.0",
    "react": "15.0.2",
    "react-bootstrap": "^0.29.5",
    "react-dom": "15.0.2",
    "react-fontawesome": "^1.1.0",
    "react-redux": "4.4.5",
    "react-router": "2.4.0",
    "react-router-redux": "4.0.4",
    "react-router-scroll": "^0.2.1",
    "react-scroll-up": "^1.1.5",
    "redux": "3.5.2",
    "redux-promise-middleware": "^3.3.2",
    "redux-thunk": "2.0.1",
    "request": "^2.72.0",
    "toastr": "2.1.2",
    "transform-loader": "^0.2.3",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-core": "6.8.0",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.1",
    "enzyme": "2.2.0",
    "eslint": "2.9.0",
    "eslint-plugin-import": "1.6.1",
    "eslint-plugin-react": "5.0.1",
    "eslint-watch": "2.1.11",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.19.0",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "image-webpack-loader": "^2.0.0",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
    "nock": "8.0.0",
    "npm-run-all": "1.8.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.0.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.0.2",
    "rimraf": "2.5.2",
