{
  "name": "React-Express-Boilerplate",
  "version": "1.0.0",
  "description": "React and Express boilerplate",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "David Urbina",
  "license": "MIT",
  "dependencies": {
    "async": "^2.0.1",
    "babel-polyfill": "^6.16.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "bootstrap-material-design": "^0.5.10",
    "cookie-parser": "^1.4.3",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "json-web-token": "^2.0.3",
    "moment": "^2.15.1",
    "mongoose": "^4.6.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.0",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-dom": "^15.3.2",
    "react-fontawesome": "^1.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-router-scroll": "^0.2.1",
    "react-scroll-up": "^1.1.5",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "request": "^2.75.0",
    "toastr": "^2.1.2",
    "transform-loader": "^0.2.3",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.16.3",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.1",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^2.0.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.1.0",
    "nock": "^8.0.0",
    "npm-run-all": "^3.1.0",
    "open": "0.0.5",
